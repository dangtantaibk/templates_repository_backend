<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">
  <mapped-superclass class="net.youthdev.vievie.entities.DataRecord">
    <attributes>
      <basic name="created">
        <column name="created" precision="20"/>
      </basic>
      <basic name="lastModified">
        <column name="last_modified" precision="20"/>
      </basic>
      <basic name="deletedOn">
        <column name="deleted_on" precision="20"/>
      </basic>
      <basic name="version">
        <column name="version" />
      </basic>
    </attributes>
  </mapped-superclass>
</entity-mappings>